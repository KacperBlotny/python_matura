import random

n = 4
print("macierz n*n przy n równym = ", n)
array = [[random.randint(0,10)for array in range(n)] for y in range(n)]

print(array)

s = 0

print("Liczby w diagonalach \ ")
for i in range(n):
    s += array[i][i]
    print(array[i][i])
print("Suma pierwszej diagonali ",s)

print("Liczby w diagonalach / ")
for j in range(n):
    s += array[j][n-j-1]
    print(array[j][n-j-1])  
print("Suma całośći ",s)

